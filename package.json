{
  "name": "facebook-ai-manager",
  "version": "2.0.0",
  "description": "AI-Powered Facebook Page Manager - Enhanced Edition",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node test-runner.js",
    "test:auth": "node -e \"require('./tests/auth.test').runAuthTests().then(result => process.exit(result ? 0 : 1))\"",
    "test:users": "node -e \"require('./tests/users.test').runUserTests().then(result => process.exit(result ? 0 : 1))\"",
    "test:facebook": "node -e \"require('./tests/facebook.test').runFacebookTests().then(result => process.exit(result ? 0 : 1))\"",
    "test:ai": "node -e \"require('./tests/ai.test').runAiTests().then(result => process.exit(result ? 0 : 1))\"",
    "test:analytics": "node -e \"require('./tests/analytics.test').runAnalyticsTests().then(result => process.exit(result ? 0 : 1))\"",
    "test:autoresponse": "node -e \"require('./tests/autoresponse.test').runAutoResponseTests().then(result => process.exit(result ? 0 : 1))\"",
    "format": "prettier --write \"src/**/*.js\" \"netlify/**/*.js\" \"tests/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" \"netlify/**/*.js\" \"tests/**/*.js\"",
    "lint:fix": "eslint --fix \"src/**/*.js\" \"netlify/**/*.js\" \"tests/**/*.js\""
  },
  "keywords": [
    "facebook",
    "ai",
    "automation",
    "social-media",
    "nodejs",
    "express"
  ],
  "author": "Facebook AI Manager Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "rate-limiter-flexible": "^8.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.4",
    "puppeteer": "^24.25.0"
  },
  "engines": {
    "node": ">=18.18.0 <19.0.0"
  }
}
